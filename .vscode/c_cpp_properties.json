{
  "version": 4,
  "configurations": [
    {
      "name": "clang++ on macOS",
      "compilerPath": "/usr/bin/clang++",
      "cStandard": "c17",
      "cppStandard": "c++17",
      "intelliSenseMode": "macos-clang-x64",
      "compileCommands": "${workspaceFolder}/build/dev/compile_commands.json",
      "includePath": ["${workspaceFolder}/include"],
      "browse": { "path": ["${workspaceFolder}/include", "${workspaceFolder}"], "limitSymbolsToIncludedHeaders": true },
      "defines": []
    }
  ]
}

